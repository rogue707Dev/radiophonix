{
    "name": "radiophonix/radiophonix",
    "description": "Plate-forme d'Ã©coute et de publication de Sagas MP3",
    "keywords": ["saga", "sagas", "mp3"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Nathan Boiron",
            "email": "nathan.boiron@gmail.com",
            "homepage": "http://mopolo.fr"
        },
        {
            "name": "Pierre TL",
            "homepage": "http://www.pierre.tl"
        },
        {
            "name": "Quoc-Viet Phan",
            "homepage": "https://gitlab.com/AzOoDev"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.7.*",
        "fideloper/proxy": "^4.0",
        "tymon/jwt-auth": "^1.0@dev",
        "league/fractal": "^0.16.0",
        "barryvdh/laravel-cors": "^0.11.2",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-medialibrary": "^7.0.0",
        "jenssegers/optimus": "^0.2.3",
        "laravel/scout": "^5.0",
        "yab/laravel-scout-mysql-driver": "^2.0.5",
        "league/color-extractor": "^0.3.2",
        "marcw/rss-writer": "^0.4.0",
        "algolia/algoliasearch-client-php": "^1.27",
        "laravel/telescope": "^1.0",
        "spatie/laravel-backup": "^5.11",
        "spatie/flysystem-dropbox": "^1.0"
    },
    "require-dev": {
        "laravel/tinker": "~1.0",
        "barryvdh/laravel-ide-helper": "^2.2",
        "doctrine/dbal": "~2.3",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "symfony/css-selector": "2.8.*|3.4.*",
        "symfony/dom-crawler": "2.8.*|3.4.*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Radiophonix\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "ide": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
